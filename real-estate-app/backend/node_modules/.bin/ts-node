#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/ts-node/dist/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/ts-node/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/node_modules:/home/saiseikyuu/Desktop/Workspace/node_modules:/home/saiseikyuu/Desktop/node_modules:/home/saiseikyuu/node_modules:/home/node_modules:/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/ts-node/dist/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/ts-node/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/node_modules:/home/saiseikyuu/Desktop/Workspace/node_modules:/home/saiseikyuu/Desktop/node_modules:/home/saiseikyuu/node_modules:/home/node_modules:/node_modules:/home/saiseikyuu/Desktop/Workspace/SILVERSJO-TECHNICAL-EXAM/real-estate-app/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin.js" "$@"
fi
